{
  "schemaVersion": 1,
  "id": "python-addon",
  "version": "${version}",
  "name": "Meteor Python Addon",
  "description": "An addon that enables python support for Meteor.",
  "authors": [
    "Cloudburst"
  ],
  "contact": {
  },
  "icon": "assets/pythonaddon/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "cloudburst.pythonaddon.PythonAddon"
    ]
  },
  "custom": {
    "jythonmc:use" : true
  },
  "mixins": [
  ],
  "depends": {
    "java": ">=16",
    "minecraft": ">=1.17",
    "meteor-client": ">0.4.3"
  }
}
